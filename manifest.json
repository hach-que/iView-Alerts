{
  "name": "iView Alerts",
  "version": "0.2.10921.0",
  "description": "An extension to notify you when new episodes are available on ABC iView.",
  "icons": { "128": "logo.png", "48": "logo48.png" },
  "background_page": "bg.html",
  "permissions": ["notifications", "tabs", "http://tviview.abc.net.au/*", "http://www.abc.net.au/*"],
  "browser_action": {
    "default_icon": "logo24.png",
    "default_title": "ABC iView",
    "default_popup": "popup.html"
  }
}